  <!-- Welcome dialog -->
  <String Id="WelcomeDlgTitle" Overridable="yes">{\WixUI_Font_Bigger}Bem-vindo ao Assistente de Instalação do [ProductName]</String>
  <String Id="WelcomeDlgDescription" Overridable="yes">O Assistente de Instalação irá instalar o [ProductName] no seu computador. Clique em Avançar para continuar ou Cancelar para sair do assistente.</String>

  <!-- License Agreement dialog -->
  <String Id="LicenseAgreementDlgTitle" Overridable="yes">{\WixUI_Font_Title}Contrato de Licença de Uso</String>
  <String Id="LicenseAgreementDlgDescription" Overridable="yes">Leia o contrato de licença a seguir antes de instalar o [ProductName].</String>
