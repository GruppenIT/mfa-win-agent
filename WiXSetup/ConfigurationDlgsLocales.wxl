  <String Id="VerifyReadyDlgInstallText" Overridable="yes">Clique em Instalar para iniciar a instalação.</String>
  <String Id="VerifyReadyDlgRepairTitle" Overridable="yes">{\WixUI_Font_Title}Pronto para reparar o [ProductName]</String>
  <String Id="VerifyReadyDlgRepairText" Overridable="yes">Clique em Reparar para iniciar a reparação.</String>
  <String Id="VerifyReadyDlgRemoveTitle" Overridable="yes">{\WixUI_Font_Title}Pronto para remover o [ProductName]</String>
  <String Id="VerifyReadyDlgRemoveText" Overridable="yes">Clique em Remover para remover o [ProductName] do seu computador.</String>

  <!-- Progress dialog -->
  <String Id="ProgressDlgTitle" Overridable="yes">{\WixUI_Font_Title}Instalando o [ProductName]</String>
  <String Id="ProgressDlgTitleInstalling" Overridable="yes">Instalando</String>
  <String Id="ProgressDlgTitleRemoving" Overridable="yes">Removendo</String>
  <String Id="ProgressDlgTitleRepairing" Overridable="yes">Reparando</String>
  <String Id="ProgressDlgStatusLabel" Overridable="yes">Status:</String>

  <!-- Exit (Finish) dialog -->
  <String Id="ExitDlgTitle" Overridable="yes">{\WixUI_Font_Bigger}Instalação Concluída</String>
  <String Id="ExitDlgDescription" Overridable="yes">O Assistente de Instalação concluiu a instalação do [ProductName] com sucesso. Clique em Concluir para sair do assistente.</String>

  <!-- Cancel dialog -->
  <String Id="CancelDlg" Overridable="yes">[ProductName]</String>
  <String Id="CancelDlgText" Overridable="yes">Tem certeza de que deseja cancelar a instalação do [ProductName]?</String>

  <!-- Error dialog -->
  <String Id="ErrorDlg" Overridable="yes">Erro de Instalação</String>

  <!-- Disk Cost dialog -->
  <String Id="DiskCostDlgTitle" Overridable="yes">{\WixUI_Font_Title}Espaço em Disco</String>
  <String Id="DiskCostDlgDescription" Overridable="yes">O espaço em disco necessário para a instalação dos recursos selecionados.</String>

  <!-- Files in Use dialog -->
  <String Id="FilesInUseDlgTitle" Overridable="yes">{\WixUI_Font_Title}Arquivos em Uso</String>
  <String Id="FilesInUseDlgDescription" Overridable="yes">Os seguintes aplicativos estão usando arquivos que precisam ser atualizados. Feche esses aplicativos e clique em Tentar novamente.</String>

  <!-- Maintenance Welcome dialog -->
  <String Id="MaintenanceWelcomeDlgTitle" Overridable="yes">{\WixUI_Font_Bigger}Assistente de Manutenção do [ProductName]</String>
  <String Id="MaintenanceWelcomeDlgDescription" Overridable="yes">O Assistente de Manutenção permite modificar, reparar ou remover o [ProductName]. Clique em Avançar para continuar.</String>

  <!-- Maintenance Type dialog -->
  <String Id="MaintenanceTypeDlgTitle" Overridable="yes">{\WixUI_Font_Title}Manutenção do [ProductName]</String>
  <String Id="MaintenanceTypeDlgDescription" Overridable="yes">Selecione a operação que deseja realizar.</String>
  <String Id="MaintenanceTypeDlgRepairButton" Overridable="yes">Re&amp;parar</String>
  <String Id="MaintenanceTypeDlgRepairText" Overridable="yes">Reparar erros de instalação do programa.</String>
  <String Id="MaintenanceTypeDlgRemoveButton" Overridable="yes">&amp;Remover</String>
  <String Id="MaintenanceTypeDlgRemoveText" Overridable="yes">Remover o [ProductName] do seu computador.</String>
  <String Id="MaintenanceTypeDlgChangeButton" Overridable="yes">&amp;Alterar</String>
  <String Id="MaintenanceTypeDlgChangeText" Overridable="yes">Alterar os recursos do programa que estão instalados.</String>

  <!-- User Exit dialog -->
  <String Id="UserExitTitle" Overridable="yes">{\WixUI_Font_Bigger}Instalação Interrompida</String>
  <String Id="UserExitDescription" Overridable="yes">A instalação do [ProductName] foi interrompida. O sistema não foi modificado. Para concluir a instalação em outro momento, execute o assistente novamente.</String>

  <!-- Fatal Error dialog -->
  <String Id="FatalErrorTitle" Overridable="yes">{\WixUI_Font_Bigger}Erro na Instalação do [ProductName]</String>
  <String Id="FatalErrorDescription1" Overridable="yes">O Assistente de Instalação do [ProductName] foi encerrado prematuramente devido a um erro.</String>
  <String Id="FatalErrorDescription2" Overridable="yes">Clique em Concluir para sair do assistente.</String>

  <!-- Downgrade error -->
  <String Id="DowngradeErrorMessage" Overridable="yes">Uma versão mais recente do [ProductName] já está instalada.</String>

  <!-- Out of Disk Space -->
  <String Id="OutOfDiskDlgTitle" Overridable="yes">{\WixUI_Font_Title}Espaço em Disco Insuficiente</String>
  <String Id="OutOfDiskDlgDescription" Overridable="yes">Não há espaço em disco suficiente. Reduza o espaço necessário ou selecione menos recursos.</String>

